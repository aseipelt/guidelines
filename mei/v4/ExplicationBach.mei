                    </beam>
                  </layer>
                </staff>
                <mordent startid="#ex-02" />
                <dir startid="#ex-02" place="below">
                  <rend fontstyle="italic">mordant.</rend>
                </dir>
              </measure>
              <measure right="dbl">
                <staff n="1">
                  <layer>
