                <staff n="2">
                  <layer>
                    <beam place="below">
                      <note dur="32" oct="5" pname="d" stem.dir="down" />
                      <note dur="32" oct="5" pname="c" stem.dir="down" />
                      <note dur="32" oct="5" pname="d" stem.dir="down" />
                      <note dur="32" oct="5" pname="c" stem.dir="down" />
                      <note dur="32" oct="5" pname="d" stem.dir="down" />
                      <note dur="16" dots="1" oct="5" pname="c" stem.dir="down" />
                    </beam>
                  </layer>
                </staff>
                <trill startid="#ex-01" glyph.num="U+E56C" />
                <dir startid="#ex-01" place="below">
                  <rend fontstyle="italic">Trillo.</rend>
